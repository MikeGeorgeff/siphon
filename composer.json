{
    "name": "georgeff/siphon",
    "description": "Siphon Micro Framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Georgeff",
            "email": "mikegeorgeff3@gmail.com"
        }
    ],
    "require": {
        "illuminate/contracts": "5.4.*",
        "illuminate/support": "5.4.*",
        "illuminate/container": "5.4.*",
        "illuminate/events": "5.4.*",
        "illuminate/database": "5.4.*",
        "illuminate/config": "5.4.*",
        "illuminate/hashing": "5.4.*",
        "illuminate/filesystem": "5.4.*",
        "illuminate/encryption": "5.4.*",
        "predis/predis": "1.1.*",
        "nesbot/carbon": "~1.2",
        "nikic/fast-route": "1.1.*",
        "league/plates": "3.3.*",
        "filp/whoops": "2.1.*",
        "vlucas/phpdotenv": "2.4.*",
        "monolog/monolog": "1.9.*",
        "dflydev/fig-cookies": "1.0.*",
        "symfony/console": "3.2.*",
        "symfony/process": "3.2.*",
        "zendframework/zend-diactoros": "1.3.*",
        "zendframework/zend-stratigility": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Siphon\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Siphon\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
